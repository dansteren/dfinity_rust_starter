{
  "canisters": {
    "graphql": {
      "build": "cargo build --target wasm32-unknown-unknown --package graphql --release",
      "candid": "canisters/graphql/src/graphql.did",
      "type": "custom",
      "wasm": "target/wasm32-unknown-unknown/release/graphql.wasm"
    },
    "frontend": {
      "dependencies": [
        "graphql"
      ],
      "frontend": {
        "entrypoint": "canisters/frontend/public/index.js"
      },
      "source": [
        "canisters/frontend/assets",
        "dist/frontend/"
      ],
      "type": "assets"
    },
    "playground": {
      "dependencies": [
        "graphql"
      ],
      "frontend": {
        "entrypoint": "canisters/playground/index.js"
      },
      "source": [
        "dist/playground/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.6.26",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
